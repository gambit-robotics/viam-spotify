{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "gambit-robotics:spotify",
  "visibility": "private",
  "url": "https://github.com/gambit-robotics/viam-spotify",
  "description": "Spotify Connect playback module - device appears as a speaker in Spotify app",
  "models": [
    {
      "api": "rdk:service:generic",
      "model": "gambit-robotics:service:spotify",
      "short_description": "Spotify Connect speaker with playback control",
      "markdown_link": "README.md"
    },
    {
      "api": "rdk:service:discovery",
      "model": "gambit-robotics:service:audio-discovery",
      "short_description": "Discover audio output devices for Spotify configuration"
    }
  ],
  "markdown_link": "README.md",
  "entrypoint": "./exec.sh",
  "build": {
    "setup": "./setup.sh",
    "build": "make module.tar.gz",
    "path": "module.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64"
    ]
  }
}
